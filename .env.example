# ============================================
# Factory Management System - Environment Variables Template
# ============================================
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# API Configuration
VITE_API_URL=https://api.protonestconnect.co
VITE_API_BASE_PATH=/api/v1/user

# WebSocket Configuration
VITE_WS_URL=wss://api.protonestconnect.co/ws

# Auto-Login Credentials (from Protonest Dashboard)
VITE_AUTH_EMAIL=your-email@example.com
VITE_AUTH_SECRET_KEY=your-secret-key-here

# Default Device ID
VITE_DEFAULT_DEVICE_ID=device9988
